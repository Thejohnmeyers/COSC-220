int main(){
	int [] arr;
	int num = 0;
	int len = 5;
	for(int i = 0; i < len;i++){
		cout << " enter a number to input into the array";
		cin >> num;
		arr[i] = num;
	}
}
double mean(int[] array, int length){
	double mean=0;
	for(int i = 0; i < length; i++){
		mean = mean + array[i];
	}
	mean = mean/length
	return mean;
}	
